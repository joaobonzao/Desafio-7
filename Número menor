package javaapplication122;

import com.sun.org.apache.xpath.internal.operations.Equals;
import java.util.Scanner;

public class JavaApplication122 {

    public static void main(String[] args) {
       Scanner ler = new Scanner(System.in);
            System.out.println("Insira dois números");
            numeroMenor(ler.nextInt(),  ler.nextInt());



        }
        public static void numeroMenor(int num1, int num2){    
           if (num1 > num2){
               System.out.println("O número menor é: " + num2);
           }else if(num1 < num2){
            System.out.println("O número menor é: " + num1);
        }else{
               System.out.println("Os números são iguais.");
           }
            



    }

     }

 package javaapplication178;

    import java.util.Scanner;

    public class JavaApplication178 {

        public static void main(String[] args) {
            Scanner ler = new Scanner(System.in);
            System.out.println("Insira dois números");
            numeroMaior(ler.nextInt(),  ler.nextInt());



        }
        public static void numeroMaior(int num1, int num2){    
           if (num1 > num2){
               System.out.println("O número maior é: " + num1);
           }else if(num1 < num2){
            System.out.println("O número maior é: " + num2);
        }else{
               System.out.println("Os números são iguais.");
           }
            



    }

     }
